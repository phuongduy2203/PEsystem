#results-body td, #results-body th {
    font-size: 12px; /* Thu nhỏ chữ */
    text-align: center; /* Canh giữa nội dung */
}

#search-results .datatable-container {
    overflow-x: auto; /* Bật cuộn ngang nếu cần */
}

.datatable-pagination ul {
    font-size: 12px; /* Thu nhỏ chữ trong phân trang */
}

/* Bật cuộn ngang cho các ô cụ thể */
.scrollable {
    overflow-x: auto; /* Bật cuộn ngang */
    white-space: nowrap; /* Không cho phép xuống dòng */
    text-overflow: ellipsis; /* Thêm dấu "..." nếu nội dung tràn */
    display: block; /* Đảm bảo ô hoạt động như block để cuộn */
}
/* Áp dụng cho các ô trong bảng */
.datatable-table th, .datatable-table td {
    white-space: nowrap; /* Không cho xuống dòng */
}


/* Mặc định: Nội dung bị cắt với dấu "..." */
.datatable-table td {
    white-space: nowrap; /* Không cho phép xuống dòng */
    overflow: hidden !important; /* Ẩn nội dung vượt quá */
    text-overflow: ellipsis; /* Thêm dấu "..." nếu nội dung bị cắt */
}

/* Khi hàng được chọn: Hiển thị đầy đủ nội dung */
.datatable-table tr.expanded td {
    max-width: none; /* Loại bỏ giới hạn chiều rộng */
    white-space: normal !important; /* Cho phép xuống dòng */
    overflow: visible !important; /* Hiển thị toàn bộ nội dung */
}




/* Ghim cột SN */
.sticky-col {
    left: 0; /* Ghim ở mép trái */
    z-index: 2; /* Đảm bảo cột SN nằm trên các ô khác */
    text-align: center; /* Căn giữa nội dung */
    font-weight: bold;
    background: white;
}
/* Ghim thanh tiêu đề (header) */
.datatable-table thead th {
    position: sticky; /* Sticky header */
    top: 0; /* Ghim ở đầu */
    background: #4CAF50; /* Màu nền */
    color: white;
    z-index: 1; /* Đảm bảo header nằm trên dữ liệu */
}


/* Đặt chiều rộng các cột đều nhau */
.datatable-table th, .datatable-table td {
    width: 140px; /* Chia đều */
    text-align: center;
    overflow: hidden; /* Ẩn nội dung khi quá dài */
    text-overflow: ellipsis; /* Hiển thị dấu "..." khi nội dung bị ẩn */
}

/* Tooltip hiển thị khi hover */
.tooltip {
    position: absolute;
    display: none;
    background: rgba(0, 0, 0, 0.8);
    color: #fff;
    padding: 8px;
    border-radius: 4px;
    font-size: 14px;
    z-index: 9999;
}

